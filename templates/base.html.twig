            <link rel="stylesheet" href="{{asset('css/style.css') }}">
        {% endblock %}

        <meta name="viewport" content="width=device-width, initial-scale=1.0">

       
    </head>
    <body>
        <nav>
            <ul>
                <li><a href="{{ path('app_formation') }}">Liste des formations </a> </li>
